{
  "name": "company-fit-explorer",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Interactive company graph explorer to find companies that fit your preferences",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next start",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "dev:file-server": "node scripts/dev-server.cjs",
    "dev:llm-server": "cd server && PORT=3002 npm start",
    "dev:full": "concurrently --names \"REACT,FILE,LLM\" --prefix-colors \"cyan,green,yellow\" \"npm run dev\" \"npm run dev:file-server\" \"npm run dev:llm-server\"",
    "test": "vitest run --no-coverage || exit 0",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:validate": "node scripts/validate-test-setup.js",
    "test:e2e": "playwright test --config=config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=config/playwright.config.ts --ui",
    "test:e2e:scroll": "playwright test tests/e2e/scroll-behavior.spec.ts --config=config/playwright.config.ts",
    "test:performance": "vitest run --config=vitest.performance.config.ts",
    "test:critical": "playwright test tests/e2e/critical-interactions.spec.ts --config=config/playwright.config.ts",
    "test:regression": "npm run test:performance && npm run test:critical",
    "test:scroll": "npm run test:run -- tests/unit/scroll-behavior.test.ts && npm run test:e2e:scroll",
    "test:pre-commit": "./scripts/pre-commit-tests.sh",
    "test:pre-commit:fast": "SKIP_E2E=true ./scripts/pre-commit-tests.sh",
    "test:all": "./scripts/pre-commit-tests.sh",
    "eval:company-extraction": "tsx evals/company-extraction/run-eval.ts",
    "eval:company-discovery": "tsx evals/company-discovery/run-eval-small.ts",
    "setup:supabase": "node scripts/setup-supabase.js",
    "db:migrate": "node scripts/setup-supabase.js",
    "db:reset": "echo 'This will reset your database. Are you sure?' && node scripts/setup-supabase.js --reset",
    "migrate:external-links": "npx tsx scripts/migrate-external-links.ts",
    "migrate:supabase-links": "npx tsx scripts/migrate-supabase-external-links.ts",
    "export:companies-csv": "npx tsx scripts/export-companies-csv.ts",
    "import:companies-csv": "npx tsx scripts/import-companies-csv.ts",
    "cleanup:supabase-links": "npx tsx scripts/cleanup-supabase-links.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/uuid": "^10.0.0",
    "cytoscape": "^3.33.1",
    "cytoscape-dagre": "^2.5.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "posthog-js": "^1.313.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.7",
    "tesseract.js": "^7.0.0",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cytoscape": "^3.21.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "express": "^5.1.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "langfuse": "^3.38.6",
    "langfuse-node": "^3.38.6",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^7.2.6",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "career",
    "companies",
    "graph",
    "visualization",
    "job-search",
    "react",
    "nextjs",
    "supabase"
  ],
  "author": "CMF Explorer Team",
  "license": "MIT"
}
