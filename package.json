{
  "name": "company-fit-explorer",
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive company graph explorer to find companies that fit your preferences",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "dev:file-server": "node dev-server.cjs",
    "dev:llm-server": "cd server && PORT=3002 npm start",
    "dev:full": "concurrently --names \"REACT,FILE,LLM\" --prefix-colors \"cyan,green,yellow\" \"npm run dev\" \"npm run dev:file-server\" \"npm run dev:llm-server\"",
    "test": "vitest run --no-coverage || exit 0",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:validate": "node scripts/validate-test-setup.js",
    "test:e2e": "playwright test --config=config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=config/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=config/playwright.config.ts --headed",
    "docs:tests": "node scripts/generate-test-docs.js"
  },
  "dependencies": {
    "cytoscape": "^3.26.0",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cytoscape": "^3.21.9",
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "typescript",
    "cytoscape",
    "company-fit",
    "companies",
    "career"
  ],
  "author": "John Smith",
  "license": "MIT"
}
